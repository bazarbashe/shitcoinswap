<h1><%= t '.admin_1' %></h1>

<table>
  <thead>
    <tr>
      <th><%= t '.user' %></th>
      <th><%= t '.asset' %></th>
      <th><%= t '.amount' %></th>
      <th><%= t '.status' %></th>
      <th><%= t '.transaction' %></th>
      <th><%= t '.error' %></th>
    </tr>
  </thead>

  <tbody>
    <% @withdrawals.each do |withdrawal| %>
      <tr>
        <td><%= withdrawal.user.email %></td>
        <td><%= withdrawal.asset.name %></td>
        <td><%= withdrawal.amount %></td>
        <td><%= withdrawal.status %></td>
        <td><%= withdrawal.transaction_id %></td>
        <td><%= withdrawal.error %></td>
      </tr>
    <% end %>
  </tbody>
</table>
